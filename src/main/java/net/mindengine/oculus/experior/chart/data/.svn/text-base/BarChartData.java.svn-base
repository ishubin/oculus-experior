package net.mind_engine.oculus.testrunframework.chart.data;

public class BarChartData
{
    private String name;
    private String color;
    
    public BarChartData()
    {
    }
    
    
    public BarChartData(String name, String color)
    {
        super();
        this.name = name;
        this.color = color;
    }


    public String getName()
    {
        return name;
    }
    public void setName(String name)
    {
        this.name = name;
    }
    public String getColor()
    {
        return color;
    }
    public void setColor(String color)
    {
        this.color = color;
    }
}
