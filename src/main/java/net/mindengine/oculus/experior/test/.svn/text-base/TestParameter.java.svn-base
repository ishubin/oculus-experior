package net.mind_engine.oculus.testrunframework.test;

import java.io.Serializable;

public class TestParameter implements Serializable
{
    private static final long serialVersionUID = 5713639726291098774L;
    private String name;
	private String value;
	
	public TestParameter()
	{
		
	}
	
	public TestParameter(String name, String value)
	{
		super();
		this.name = name;
		this.value = value;
	}

	public String getName()
	{
		return name;
	}
	public void setName(String name)
	{
		this.name = name;
	}
	public String getValue()
	{
		return value;
	}
	public void setValue(String value)
	{
		this.value = value;
	}

}
